<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Line?>

<VBox fx:id="rootLayout" prefHeight="480.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.EniacDevelopment.ControlClient.view.RootLayoutController">
   <children>
      <Pane prefHeight="45.0" prefWidth="800.0">
         <children>
            <Label text="Eniac Development Group">
               <padding>
                  <Insets left="50.0" top="4.0" />
               </padding></Label>
            <Label text="Time">
               <padding>
                  <Insets left="750.0" right="5.0" top="4.0" />
               </padding>
            </Label>
            <ImageView fitHeight="25.0" fitWidth="44.0" pickOnBounds="true" preserveRatio="true" />
         </children>
      </Pane>
      <Line endX="800.0" />
      <StackPane fx:id="sceneHolder" prefHeight="420.0" prefWidth="800.0" />
      <ImageView fitHeight="20.0" fitWidth="800.0" pickOnBounds="true" preserveRatio="true" />
      <ButtonBar buttonMinWidth="240.0" prefHeight="40.0" prefWidth="200.0">
        <buttons>
          <Button mnemonicParsing="false" onAction="#handleSecurityButton" text="Security" />
            <Button mnemonicParsing="false" onAction="#handleSensorButton" text="Sensors" />
            <Button mnemonicParsing="false" onAction="#handleUserButton" text="Users" />
        </buttons>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" />
         </padding>
      </ButtonBar>
   </children>
</VBox>
